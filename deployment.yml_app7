apiVersion: apps/v1
kind: Deployment
metadata: 
  name: my-app7
spec:
  replicas: 2
  selector:
    matchLabels:
      app: my-app7
  template:
    metadata:
      labels:
        app: my-app7
    spec:
      containers:
        - name: my-container
          image: nginx
          ports:
            - containerPort: 80
